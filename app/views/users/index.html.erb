<header>
    <% if !current_user %>
        <div class="static"><%= link_to "Log In", login_path %></div>
        <div class="static"><%= link_to "Sign Up", new_user_path %></div>
    <% end %>
</header>

<h1>Check out all of our amazing artists!</h1>

<% flash.each do |type, msg| %>
    <div class="alert"><b><%= msg %></b></div><br>
<% end %>

<% @users.each do |user| %>
    <%= link_to user.full_name, user_path(user) %><br>
<% end %>